{
  "Ubuntu": [
    {
      "name": ".gitconfig",
      "src": "git",
      "destination": "$HOME"
    },
    {
      "name": ".gitmessage.txt",
      "src": "git",
      "destination": "$HOME"
    },
    {
      "name": "config.fish",
      "destination": "$HOME/.config/fish"
    },
    {
      "name": "gpg-agent.conf",
      "destination": "$HOME/.gnupg"
    }
  ],
  "MacOSX": [
    {
      "name": ".gitconfig",
      "src": "git",
      "destination": "$HOME"
    },
    {
      "name": ".gitmessage.txt",
      "src": "git",
      "destination": "$HOME"
    },
    {
      "name": "config.fish",
      "destination": "$HOME/.config/fish"
    },
    {
      "name": "settings.json",
      "src": "vscode",
      "destination": "$HOME/Library/Application Support/Code/User"
    },
    {
      "name": "gpg-agent.conf",
      "destination": "$HOME/.gnupg"
    }
  ]
}